train_net: "models/progressiros/VGG16/faster_rcnn_end2end/train.prototxt"
base_lr: 0.0008
lr_policy: "step"
gamma: 0.5
stepsize: 5000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_faster_rcnn"
iter_size: 2
